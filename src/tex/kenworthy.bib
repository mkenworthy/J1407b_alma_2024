@article{Haguenauer06,
  title        = {Deep nulling of laser light with a single-mode-fiber beam combiner},
  author       = {Pierre Haguenauer and Eugene Serabyn},
  year         = 2006,
  month        = {Apr},
  journal      = {Appl. Opt.},
  publisher    = {Optica Publishing Group},
  volume       = 45,
  number       = 12,
  pages        = {2749--2754},
  doi          = {10.1364/ao.45.002749},
  url          = {https://opg.optica.org/ao/abstract.cfm?URI=ao-45-12-2749},
  keywords     = {Spatial filtering; Interferometry; Astronomical optics; Beam splitters; Laser light; Light beams; Phase shift; Polarization control; Single mode fibers},
  abstract     = {The detection of faint companions to bright stars requires the development of very-high-contrast-ratio detection techniques such as nulling interferometry. Here we present, to our knowledge, the first experimental test of a novel beam-combination method, which is based on coupling the light at the center of a dual-aperture interference pattern into a single-mode fiber. Using such a "fiber nuller" approach, a visible He-Ne laser has been stably nulled to as low as1.3{\texttimes}10{\textminus}6, thus validating the approach.}
}

