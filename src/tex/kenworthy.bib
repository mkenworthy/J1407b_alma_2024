@article{Mawet11b,
  title        = {Improved high-contrast imaging with on-axis telescopes using a multistage vortex coronagraph},
  author       = {Dimitri Mawet and Eugene Serabyn and J. Kent Wallace and Laurent Pueyo},
  year         = 2011,
  month        = {Apr},
  journal      = {Opt. Lett.},
  publisher    = {Osa},
  volume       = 36,
  number       = 8,
  pages        = {1506--1508},
  doi          = {10.1364/ol.36.001506},
  url          = {http://ol.osa.org/abstract.cfm?URI=ol-36-8-1506},
  keywords     = {Spatial filtering; Telescopes; Astronomical optics},
  abstract     = {The vortex coronagraph is one of the most promising coronagraphs for high-contrast imaging because of its simplicity, small inner working angle, high throughput, and clear off-axis discovery space. However, as with most coronagraphs, centrally obscured on-axis telescopes degrade contrast. Based on the remarkable ability of vortex coronagraphs to move light between the interior and exterior of pupils, we propose a method based on multiple vortices, that without sacrificing throughput, reduces the residual light leakage to (a/A)n, with n$\geq$4, and a and A being the radii of the central obscuration and primary mirror, respectively. This method thus enables high contrasts to be reached even with an on-axis telescope.}
}

